    "@ice/runtime": "^1.2.7",
    "@swc/types": "^0.1.12",
    "@tauri-apps/plugin-http": "~2",
    "animate.css": "^4.1.1",
    "antd": "^5.0.0",
    "classnames": "^2.5.1",
    "clipboardy": "^4.0.0",
    "cropperjs": "^1.6.2",
    "current-device": "^0.10.2",
    "enquire-js": "^0.2.1",
    "html2canvas": "^1.4.1",
    "katex": "^0.16.21",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "qrcode": "^1.5.3",
    "qrcode.react": "^4.0.1",
    "rc-queue-anim": "^2.0.0",
    "rc-scroll-anim": "^2.7.6",
    "rc-tween-one": "^3.0.6",
    "react": "^18.2.0",
    "react-cropper": "^2.3.3",
    "react-dom": "^18.2.0",
