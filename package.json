{
  "name": "OfferWing",
  "version": "1.2.6",
  "description": "OfferWing - 无需准备，轻松拿到offer",
  "scripts": {
    "dev": "ice start --speedup --no-open",
    "start": "ice start --speedup",
    "prebuild": "node ./generateVersion.js",
    "build": "ice build",
    "eslint": "eslint ./src --cache --ext .js,.jsx,.ts,.tsx",
    "eslint:fix": "npm run eslint -- --fix",
    "stylelint": "stylelint \"src/**/*.{css,scss,less}\" --cache",
    "stylelint:fix": "npm run stylelint -- --fix",
    "tauri": "tauri",
    "release": "bumpp -y --all",
    "dev:ios": "tauri ios dev 'iPhone 16'",
    "dev:android": "tauri android dev",
    "build:android": "tauri android build --apk",
    "build:ios": "pnpm tauri ios build --export-method app-store-connect",
    "android:sign": "keytool -genkey -v -keystore ./offerWing.jks -keyalg RSA -keysize 2048 -validity 10000 -alias offerWing"
  },
  "dependencies": {
    "@ant-design/charts": "^1.4.2",
