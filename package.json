    "@ice/plugin-auth": "^1.0.0",
    "@ice/plugin-request": "^1.0.0",
    "@ice/plugin-store": "^1.0.0",
    "@release-it/conventional-changelog": "^7.0.2",
    "@swc/core": "^1.5.7",
    "@tauri-apps/cli": "^2.4.0",
    "@types/node": "^18.11.17",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "bumpp": "^10.1.0",
    "chokidar": "^3.6.0",
